<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.jboss.reddeer.codegen"
            name="RedDeer CodeGen"
            parentCategory="org.jboss.reddeer.tools">
      </category>
      <wizard
            category="org.jboss.reddeer.codegen"
            class="org.jboss.reddeer.codegen.wizards.CodeGenWizard"
            icon="icons/reddeer_icon.png"
            id="org.jboss.reddeer.codegen.wizards.CodeGenWizard"
            name="CodeGen">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.jboss.reddeer.codegen.toolbars.codeGenToolbar"
               label="RedDeer CodeGen">
            <command
                  commandId="org.jboss.reddeer.codegen.commands.codeGenCommand"
                  icon="icons/reddeer_icon.png"
                  id="org.jboss.reddeer.codegen.toolbars.codeGenCommand"
                  label="Red Deer CodeGen"
                  tooltip="Red Deer CodeGen">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.jboss.reddeer.codegen.commands.category"
            name="CodeGen Category">
      </category>
      <command
            categoryId="org.jboss.reddeer.codegen.commands.category"
            id="org.jboss.reddeer.codegen.commands.codeGenCommand"
            name="CodeGen Command">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.jboss.reddeer.codegen.handlers.CodeGenHandler"
            commandId="org.jboss.reddeer.codegen.commands.codeGenCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.jboss.reddeer.codegen.commands.codeGenCommand"
            contextId="org.eclipse.ui.contexts.dialogAndWindow"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M3+G">
      </key>
   </extension>

</plugin>
